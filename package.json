{
  "name": "jektut",
  "version": "1.0.0",
  "description": "This is a simple Jekyll boilerplate I am building to have a starting point for Jekyll projects. It includes Bootstrap styling.",
  "main": "index.js",
  "scripts": {
    "build-gulp": "rm -fr production && bundle exec jekyll build && node index.js && gulp",
    "compile-images": "node index.js",
    "serve": "bundle exec jekyll serve --livereload --trace --port 4000",
    "sass": "node-sass --watch webpack/sass -o assets/css",
    "upload": "_scripts/upload.sh",
    "webpack": "webpack",
    "build": "bundle exec jekyll build && gulp",
    "rm-production": "node scripts/rmProduction.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donniecode1983/jekyll-bolierplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/donniecode1983/jekyll-bolierplate/issues"
  },
  "homepage": "https://github.com/donniecode1983/jekyll-bolierplate#readme",
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-terser": "^2.1.0",
    "image-size": "^1.0.0",
    "node-sass": "^7.0.1",
    "sharp": "^0.30.7",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.0"
  }
}
